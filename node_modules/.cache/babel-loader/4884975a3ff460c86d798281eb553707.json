      this.collapsed = true;\r\n      }\r\n    },\r\n    sliderChange: function(value) {\r\n      this.opacity = value\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      'clamp'\r\n    ]),\r\n    textActive: function() {\r\n      if(this.size > 150) {\r\n        return true\r\n      }\r\n      return false\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n* {\r\n  color: rgb(221, 221, 221);\r\n  font-family: Comfortaa;\r\n}\r\n.sidebar-right {\r\n  display: flex;\r\n  pointer-events: all;\r\n}\r\n.resize {\r\n  background-color: rgb(94, 94, 94);\r\n  flex: 0 0 auto;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-content: flex-start;\r\n  z-index: 1;\r\n  pointer-events: all;\r\n}\r\n@media screen and (max-width: 992px) {\r\n  .resize {\r\n    display: none !important;\r\n  }\r\n}\r\n.handler {\r\n  width: 6px;\r\n  background-color: rgb(63, 63, 63);\r\n  border-top: 4px solid #6aa2f7;\r\n  cursor: e-resize;\r\n  z-index: 1;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n\r\n  -moz-user-select: none;\r\n  -webkit-user-select: none;\r\n  user-select: none;\r\n}\r\n.top-bar {\r\n  height: 30px;\r\n  width: 100%;\r\n  background-color: rgb(77, 77, 77);\r\n  border-top: 4px solid rgb(106, 162, 247);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: flex-start;\r\n}\r\n.collapse-btn {\r\n  height: 14px;\r\n  width: 20px;\r\n  background-color: rgb(77, 77, 77);\r\n  margin-bottom: 1px;\r\n  border-radius: 3px;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.collapse-btn:hover {\r\n  background-color: rgb(99, 99, 99);\r\n}\r\n.collapse-btn:active {\r\n  background-color: rgb(61, 61, 61);\r\n}\r\n.flip {\r\n  -webkit-transform: scaleX(-1);\r\n  transform: scaleX(-1);\r\n}\r\n\r\n.tab-properties {\r\n  width: 100%;\r\n  flex: 0 0 auto;\r\n}\r\n.propert